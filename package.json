{
  "name": "vue-learning",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "start:dev": "NODE_ENV=development vite",
    "start:prod": "NODE_ENV=production vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-query": "^1.26.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@pinia/testing": "^0.1.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-actions": "^7.0.26",
    "@storybook/addon-docs": "^7.0.26",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/addon-mdx-gfm": "^7.0.26",
    "@storybook/cli": "^7.0.26",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.0.26",
    "@storybook/vue3-vite": "^7.0.26",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "babel-loader": "^8.3.0",
    "cypress": "^12.12.0",
    "eslint": "^8.39.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.63.2",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.0.26",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "volar-service-vetur": "^0.0.7",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.6.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
